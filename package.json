{
  "name": "udea-mock-backend",
  "version": "1.0.0",
  "description": "Drive API Node Js",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "start": "npm run build && node ./build/index.js",
    "dev": "export && nodemon --watch src -e ts,js --exec npm start",
    "test": "mocha --require ts-node/register 'test/**/*.spec.ts'",
    "coverage": "nyc _mocha --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && http-server coverage/",
    "sonar": "npm run coverage && npm run coverage:report && gulp sonar",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.ts\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.ts\"",
    "prettier": "prettier --check \"./{test,src}/**/*.ts\"",
    "debug": "node ./build/index.js"
  },
  "author": "Felipe Garzon",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^72.0.0",
    "mime-types": "^2.1.30",
    "msnodesqlv8": "^2.0.11",
    "multer": "^1.4.2",
    "multiparty": "^4.2.2",
    "sinon": "^9.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.11",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5.2.7",
    "@types/mssql": "^6.0.7",
    "@types/multer": "^1.4.5",
    "@types/multiparty": "0.0.32",
    "@types/node": "^12.19.16",
    "@types/uuid": "^3.4.9",
    "@types/yup": "^0.29.9",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "gulp": "^4.0.2",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "2.1.1",
    "repack-zip": "^0.2.8",
    "sonarqube-scanner": "^2.5.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.9.9"
  }
}
